{
  "name": "crawler-ntust-course",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "files": [
    "bin/",
    "dist/"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint src test",
    "check": "npm run lint -s && dependency-check package.json --entry src",
    "watch": "watch 'npm run build' src test",
    "test": "mocha --compilers js:babel/register",
    "console": "babel-node bin/console",
    "prebuild": "npm run check -s && npm run clean -s",
    "build": "babel --optional runtime src -d dist",
    "postbuild": "npm run test -s",
    "prepublish": "npm run build -s",
    "deploy": "git pull --rebase origin master && git push origin master",
    "patch": "npm version patch && npm publish",
    "minor": "npm version minor && npm publish",
    "major": "npm version major && npm publish",
    "postpublish": "git push origin master --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/colorgy/crawler-ntust-course-js.git"
  },
  "keywords": [
    "node"
  ],
  "author": "Neson <neson@dex.tw>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/colorgy/crawler-ntust-course-js/issues"
  },
  "homepage": "https://github.com/colorgy/crawler-ntust-course-js#readme",
  "devDependencies": {
    "babel": "*",
    "babel-eslint": "*",
    "chai": "^3.3.0",
    "coveralls": "*",
    "dependency-check": "*",
    "doctoc": "*",
    "eslint": "*",
    "isparta": "*",
    "mocha": "^2.3.3",
    "rimraf": "*",
    "watch": "*"
  },
  "dependencies": {
    "babel-runtime": "*",
    "cheerio": "^0.19.0",
    "request": "^2.64.0",
    "request-promise": "^0.4.3"
  }
}
